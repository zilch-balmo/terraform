[
    {
	"cpu": ${cpu},
	"essential": true,
	"image": "nginx:latest",
	"memory": ${memory},
	"name": "backend",
	"portMappings": [
            {
		"containerPort": 80,
		"hostPort": 80
            }
	],
	"logConfiguration": {
	    "logDriver": "awslogs",
	    "options": {
		"awslogs-group": "${log_group}",
		"awslogs-region": "us-west-2",
		"awslogs-stream-prefix": "ecs"
	    }
	}
    }
]
